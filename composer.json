{
  "name": "orchestra/asset",
  "description": "Asset Component for Orchestra Platform",
  "homepage": "http://orchestraplatform.com/docs/latest/components/asset/",
  "keywords": [
    "orchestra-platform",
    "orchestral",
    "laravel",
    "asset"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mior Muhammad Zaki",
      "email": "crynobone@gmail.com",
      "homepage": "https://github.com/crynobone"
    }
  ],
  "autoload": {
    "psr-4": {
      "Orchestra\\Asset\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "laravie/html": "8.x-dev",
    "orchestra/contracts": "8.x-dev",
    "orchestra/support-facades": "8.x-dev"
  },
  "require-dev": {
    "illuminate/filesystem": "^10.0",
    "larastan/larastan": "^2.0",
    "orchestra/testbench": "^8.0"
  },
  "suggest": {
    "illuminate/filesystem": "Allow using orchestra/asset component outside of Laravel app (^9.0).",
    "orchestra/html": "Allow using orchestra/asset with HTML component (^7.0)."
  },
  "extra": {
    "branch-alias": {
      "dev-master": "8.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev" ,
  "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/katsana-external/laravie-html"
    },
    {
      "type": "vcs",
      "url": "https://github.com/katsana-external/contracts"
  },
  {
    "type": "vcs",
    "url": "https://github.com/katsana-external/support-facades"
  }
  ]
}
